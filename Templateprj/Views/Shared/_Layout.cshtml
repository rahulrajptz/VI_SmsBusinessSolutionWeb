<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no" name="viewport">
    <meta name="format-detection" content="telephone=no" />
    <meta name="theme-color" content="#eed415" />
    <meta name="msapplication-navbutton-color" content="#eed415">
    <meta name="apple-mobile-web-app-status-bar-style" content="#eed415">
    <meta http-equiv="X-UA-.
          atible" content="IE=edge,chrome=1">
    <link rel="icon" href="~/assets/img/BR.png" type="image/png" />
    <title>Template</title>
    @*//font and icons*@
    @*https://fontawesome.com/icons?d=gallery  ---- icon url*@

    <link rel="stylesheet" type='text/css' href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.8/css/all.css" integrity="sha384-3AB7yXWz4OeoZcPbieVW64vVXEwADiYyAEhwilzWsLw+9FgqpyjjStpPnpBO8o8S" crossorigin="anonymous">
    @*<link href="~/Content/static/css/bootstrap.min.css" rel="stylesheet" />*@
    @*<link href="~/Content/static/css/now-ui-dashboard.min.css" rel="stylesheet" />*@
    @*<link href="~/Content/static/css/demo.css" rel="stylesheet" />*@
    @*<link rel="stylesheet" type='text/css' href="https://cdn.datatables.net/buttons/1.5.1/css/buttons.dataTables.min.css">*@
    @*<link href="~/Content/select2.css" rel="stylesheet" />*@
    @*<link href="~/Content/select2-bootstrap.css" rel="stylesheet" />*@
    <base href="@Url.Content("~")" />
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/Font")
    @Styles.Render("~/Content/Cusom")
    @RenderSection("styles", required: false)
</head>
<body>
    <link href="~/assets/css/CustomCss.css" rel="stylesheet" />
    <noscript>
        <div class="JSError">
            Please Enable Javascript and Refresh the Page
        </div>
    </noscript>
    <div class="sidebar"></div>
    <div class="wrapper">
        <div class="sidebar" data-color="orange">
            <div class="logo">
                <a class="simple-text logo-mini" href="#">
                    TM
                </a>
                <a href="#" class="simple-text logo-normal">
                    Template
                </a>
                <div class="navbar-minimize">
                    <button id="minimizeSidebar" class="btn btn-simple btn-icon btn-neutral btn-round">
                        <i class="fa fa-bars visible-on-sidebar-regular"></i>
                        <i class="fa fa-angle-double-right visible-on-sidebar-mini"></i>
                    </button>
                </div>
            </div>
            @Html.Partial(MVC.Shared.Views._DeskNavLinks);
        </div>
        <div class="main-panel ps-container ps-theme-default" data-ps-id="0e902cce-0c39-23e4-7594-d92ed7a3c672">
            <!-- Navbar -->
            <nav class="navbar navbar-expand-lg navbar-transparent  navbar-absolute bg-secondary fixed-top">
                <div class="container-fluid">
                    <div class="navbar-wrapper">
                        <div class="navbar-toggle">
                            <button type="button" class="navbar-toggler">
                                <span class="navbar-toggler-bar bar1"></span>
                                <span class="navbar-toggler-bar bar2"></span>
                                <span class="navbar-toggler-bar bar3"></span>
                            </button>
                        </div>
                        <a class="navbar-brand" href="#"></a>
                    </div>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navigation" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-bar navbar-kebab"></span>
                        <span class="navbar-toggler-bar navbar-kebab"></span>
                        <span class="navbar-toggler-bar navbar-kebab"></span>
                    </button>
                    <div class="collapse navbar-collapse justify-content-end" id="navigation">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link dropdown-toggle" href="http://example.com" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <i class="now-ui-icons users_single-02"></i>
                                    <p>
                                        <span class="d-lg-none d-md-block">Some Actions</span>
                                    </p>
                                </a>
                                <div class="dropdown-menu dropdown-menu-right col-md-4" aria-labelledby="navbarDropdownMenuLink" style="width:300px ; height:100px">
                                    <div class="card card-user">
                                        <div class="image">
                                            <img src="~/assets/img/bg14.jpg" alt="..." />
                                        </div>
                                        <div class="card-body">
                                            <div class="author">
                                                @*<a href="#">*@
                                                <img class="avatar border-gray" src="~/assets/img/james.jpg" alt="...">
                                                <h5 class="title">Mike Andrew</h5>
                                                @*</a>*@
                                                <a href="@Url.Action("Login", "Account")" class="dropdown-item waves-effect waves-orange not-close full-width"><i class="now-ui-icons arrows-1_minimal-left"></i> Logout</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
            <!-- End Navbar -->
            <div class="panel-header panel-header-sm">
            </div>
            <div class="content">
                @RenderBody()
            </div>
            <footer class="footer">
                <div class="container-fluid">
                </div>
            </footer>
            <div class="ps-scrollbar-x-rail" style="left: 0px; bottom: 0px;"><div class="ps-scrollbar-x" tabindex="0" style="left: 0px; width: 0px;"></div></div><div class="ps-scrollbar-y-rail" style="top: 0px; right: 0px;"><div class="ps-scrollbar-y" tabindex="0" style="top: 0px; height: 0px;"></div></div>
        </div>
    </div>

    @Scripts.Render("~/Scripts/CommonJS")
    @RenderSection("scripts", required: false)


    <script src="~/assets/session/jquery-ui.min.js"></script>
    <link href="~/assets/session/timeout-dialog.css" rel="stylesheet" />
    <script src="~/assets/session/timeout-dialog.js"></script>
    <script>
    $(document).ready(function () {
        $(function () {
            var fnTimeOut = function () {
                jQuery.timeoutDialog.setupDialogTimer({
                    timeout: 1200,
                    countdown: 20,
                    logout_redirect_url: '@Url.Action("Login", "Account")',
                    keep_alive_url: '@Url.Action("Keepalive", "Home")'
               });
            };
            fnTimeOut();
        });
    });
    </script>

</body>
</html>
